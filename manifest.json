{
  "name": "Breadcrumbs",
  "version": "1.0",
  "description": "Simplify your browsing experience and find only the things you really need.",
  "permissions": ["activeTab", "declarativeContent", "storage", "tabs"],
  "web_accessible_resources": [
    "widget.css",
    "modal.html",
    "websites.js",
    "bootstrap.min.css"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}